{
  "name": "Gabo Power Tools",
  "description": "A collection of random Sketch tools I've built for my personal use.",
  "author": "Gabo Mendoza",
  "homepage": "https://github.com/gabocorp/SketchGPT",
  "appcast": "https://raw.githubusercontent.com/gabocorp/SketchGPT/master/.appcast.xml",
  "identifier": "com.gabocorp.sketchgpt",
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "commands": [
    {
      "script": "symboltree.js",
      "shortcut": "cmd option control 0",
      "handler": "onSyncSymbolTree",
      "name": "SymbolTree (Organize + Rename)",
      "identifier": "syncsymbols"
    },
    {
      "script": "auditor.js",
      "shortcut": "cmd option control -",
      "handler": "onGenerateAuditor",
      "name": "Make TextStyle Samples",
      "identifier": "generateauditor"
    },
    {
      "script": "auditor.js",
      "handler": "onSaveStyleJSON",
      "name": "Export Shared TextStyles to JSON file",
      "identifier": "savejson"
    }
    ,
    {
      "script": "gonext.js",
      "shortcut": "cmd option control .",
      "handler": "onGoNext",
      "name": "Select Next Instance...",
      "identifier": "gonext"
    }
  ],
  "menu": {
    "title" : "⚡️ Gabo Power Tools",
    "items": [
      "syncsymbols", 
      "-",
      "generateauditor",
      "savejson",
      "-",
      "gonext"
    ]
  }
}